imaryFonts: (\r\n  \"bold\": \"open_sansbold\",\r\n  \"regular\": \"open_sansregular\",\r\n  \"light\": \"open_sanslight\",\r\n);\r\n\r\n$backgroundColor: #f0efe9;\r\nbody {\r\n  background-color: $backgroundColor;\r\n  font-family: map.get($primaryFonts, \"regular\");\r\n}\r\n\r\n.container {\r\n  @include flex(column);\r\n  @include alignMainAndMinorAxis(center, center);\r\n  @include setWidthAndHeightToElements(auto, 100vh);\r\n}\r\n\r\n%btnSave {\r\n  margin-top: 1rem;\r\n  border: none;\r\n  background-color: white;\r\n  color: #06d001;\r\n  letter-spacing: 0.1rem;\r\n  text-transform: uppercase;\r\n  border-radius: 0.2rem;\r\n  padding: 0.5rem;\r\n  align-self: flex-end;\r\n  &:hover {\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n$containerWidth: 40rem;\r\n$containerHeight: 55rem;\r\n$todoAppTextColor: #616473;\r\n.todoApp {\r\n  // @include flex(column);\r\n  // @include alignMainAndMinorAxis(space-between, flex-start);\r\n  font-size: 1.5rem;\r\n  @include setWidthAndHeightToElements($containerWidth, $containerHeight);\r\n  position: relative;\r\n  // outline: dotted red;\r\n  margin: auto;\r\n  color: $todoAppTextColor;\r\n  background-color: white;\r\n  padding: 3rem 4rem;\r\n  // border-radius: 5px;\r\n  box-shadow: 0 0 1px whitesmoke;\r\n  .todoApp__header {\r\n    // outline: dotted dodgerblue;\r\n    @include setWidthAndHeightToElements(100%);\r\n    @include flex(row);\r\n    @include alignMainAndMinorAxis(space-between, center);\r\n\r\n    .todoApp-date {\r\n      @include flex(row);\r\n      @include alignMainAndMinorAxis(center, center);\r\n    }\r\n    $letterSpacing: 0.1rem;\r\n    .todoApp-month-year {\r\n      text-transform: uppercase;\r\n      .month {\r\n        font-weight: bold;\r\n        letter-spacing: $letterSpacing;\r\n        margin: 0 {\r\n          bottom: 0.2rem;\r\n        }\r\n      }\r\n    }\r\n    .todoApp-day--number {\r\n      font-family: map.get($primaryFonts, \"bold\");\r\n      font-size: 5rem;\r\n      margin: 0 {\r\n        top: 2rem;\r\n        bottom: 2rem;\r\n        right: 1.5rem;\r\n      }\r\n    }\r\n\r\n    .todoApp-day--string {\r\n      text-transform: uppercase;\r\n      font-weight: bold;\r\n      letter-spacing: $letterSpacing;\r\n    }\r\n  }\r\n\r\n  .todoApp__footer {\r\n    @include flex(row);\r\n    @include alignMainAndMinorAxis(space-between, center);\r\n    width: 80%;\r\n    position: absolute;\r\n    top: 93%;\r\n    text-transform: capitalize;\r\n    .active {\r\n      border-bottom: 1px solid rgb(31, 231, 57);\r\n    }\r\n    .task-tab {\r\n      @include cursorHover(pointer);\r\n    }\r\n  }\r\n  .form {\r\n    @include flex(column);\r\n    @include alignMainAndMinorAxis(center, center);\r\n    background-color: rgba(#000, 0.5);\r\n    // outline: dotted dodgerblue;\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 100%;\r\n    top: 0;\r\n    left: 0;\r\n    z-index: 1;\r\n    .addingTask {\r\n      @include flex(column);\r\n      @include alignMainAndMinorAxis(center, flex-start);\r\n      border-radius: 1rem;\r\n      padding: 2rem;\r\n      margin-top: 3rem;\r\n      background-color: white;\r\n      width: 95%;\r\n      height: 35%;\r\n      label {\r\n        font-size: 2rem;\r\n      }\r\n      input[type=\"text\"] {\r\n        margin: 1rem 0;\r\n        height: 5rem;\r\n        border-radius: 0.5rem;\r\n        border: 1px solid $backgroundColor;\r\n        width: 100%;\r\n        &:focus {\r\n          border: 1px solid;\r\n          border-image: linear-gradient(45deg, #02a2f7, #06d001) 45;\r\n          outline: none;\r\n        }\r\n      }\r\n      ::placeholder {\r\n        padding-left: 1rem;\r\n      }\r\n      .btn__save {\r\n        @extend %btnSave;\r\n      }\r\n    }\r\n  }\r\n}\r\n.btn {\r\n  img {\r\n    width: 50px;\r\n    height: 50px;\r\n    &:hover {\r\n      cursor: pointer;\r\n    }\r\n  }\r\n  border: none;\r\n  position: absolute;\r\n  top: 95.5%;\r\n  left: 44%;\r\n  z-index: 2;\r\n  background-color: rgba($backgroundColor, 0);\r\n  &.btn--active {\r\n    transform: rotate(45deg);\r\n  }\r\n}\r\n\r\n.todoApp__body {\r\n  // filter: blur(4px);\r\n  .task__list {\r\n    list-style: none;\r\n    padding: 0;\r\n    margin-top: 3rem;\r\n    font-size: 2.1rem;\r\n  }\r\n\r\n  .task {\r\n    // outline: dotted dodgerblue;\r\n    @include flex(row);\r\n    @include alignMainAndMinorAxis(space-between, center);\r\n    padding: 1rem 0;\r\n    margin: 1rem 0;\r\n    input[type=\"checkbox\"] {\r\n      accent-color: #94ffd8;\r\n      // background-color: white;\r\n      @include setWidthAndHeightToElements(1.5rem, 1.5rem);\r\n\r\n      &:hover {\r\n        cursor: pointer;\r\n      }\r\n    }\r\n  }\r\n\r\n  .task-checked {\r\n    text-decoration: line-through;\r\n    font-family: map.get($primaryFonts, light);\r\n  }\r\n}\r\n\r\n.hide {\r\n  visibility: hidden;\r\n}\r\n$iconSize: 28px;\r\n.task-finished {\r\n  img {\r\n    @include setWidthAndHeightToElements($iconSize, $iconSize);\r\n  }\r\n}\r\n\r\n.task-edit {\r\n  img {\r\n    @include setWidthAndHeightToElements($iconSize, $iconSize);\r\n    margin: 0 {\r\n      left: 0.9rem;\r\n    }\r\n    @include cursorHover(pointer);\r\n  }\r\n}\r\n\r\n// .form input[type=\"text\"]:focus {\r\n//   border: 1px solid;\r\n//   outline: none;\r\n//   transition: border 0.3s ease-in-out;\r\n// }\r\n\r\n.edit-btn {\r\n  @extend %btnSave;\r\n  margin: 0;\r\n  color: #0ebbda;\r\n}\r\n\r\n.task-edit {\r\n  &.task-edit--active {\r\n    border: 1px solid #02a2f7;\r\n    border-radius: 1rem;\r\n  }\r\n  input[type=\"text\"] {\r\n    padding: 0 {\r\n      left: 0.5rem;\r\n    }\r\n    outline: none;\r\n    border: none;\r\n    border-radius: 3px;\r\n\r\n    height: 100%;\r\n    &:focus {\r\n      outline: none;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"bfe694e607b7699d86a08322c2f13d49","cacheData":{"env":{}}}